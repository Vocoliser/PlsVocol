{
	"manifest_version": 3,
	"name": "Pls i want pick cotton",
	"description": "Pls i want pick cotton",
	"version": "0.2.0",
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.roblox.com/games/8737602449/PLS-DONATE*",
				"https://web.roblox.com/games/8737602449/PLS-DONATE*"
			],
			"js": ["content/loader.js"],
			"run_at": "document_idle"
		}
	],
	"host_permissions": [
		"https://www.roblox.com/*",
		"https://web.roblox.com/*",
		"https://plsbrainrot.me/*",
		"wss://plsbrainrot.me/*",
		"https://api.github.com/*",
		"https://raw.githubusercontent.com/*"
	],
	"permissions": [
		"storage",
		"scripting"
	],
	"web_accessible_resources": [
		{
			"resources": ["remote/*", "sound.ogg"],
			"matches": ["https://www.roblox.com/*", "https://web.roblox.com/*"]
		}
	]
}
